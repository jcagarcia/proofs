type Travel {
    id: ID!
    name: String!
    country: String
    layers: [Layer]
}

type Layer {
    id: ID!
    name: String!
    travel: Travel
}

type Query {
    findAllTravels: [Travel]
    findAllLayers: [Layer]
}

type Mutation {
    createTravel(name: String!, country: String!) : Travel!
    createLayer(travel: Int!, name: String! ) : Layer!
}

schema {
    query: Query
    mutation: Mutation
}